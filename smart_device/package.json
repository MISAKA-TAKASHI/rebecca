			"src/Common/**/*.js"
		],
		"moduleFileExtensions": [
			"js"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!react-native|react-navigation|react-native-fetch-blob)/"
		]
	},
	"rnpm": {
		"assets": [
			"./assets/fonts/"
		]
	}
